	<subDataset name="alumnos" uuid="1167a8b2-b4ce-4f9c-8044-7bfb0fc5c0d8">
		<parameter name="clavefamiliarid" class="java.lang.Integer" isForPrompting="false"/>
		<queryString>
			<![CDATA[select
  a.matricula as matricula,
  CONCAT_WS(' ', a.apellidopaterno, a.apellidomaterno,a.primernombre, a.segundonombre) as nombre,
  ae.Nombre as estatus,
  n.Nombre as nivel,
  g.Grado as grado,
  gr.Nombre as grupo,
 sum(d.saldo - d.descuento) + sum(calculaInteres(d.documentoporpagarid)) as saldo
from ce_alumnoporclavefamiliar acf
inner join ce_alumno a on acf.AlumnoId = a.AlumnoId
inner join ce_alumnoestatus ae on a.AlumnoEstatusId = ae.AlumnoEstatusId

inner join ce_alumnoporciclo ac on a.AlumnoId = ac.AlumnoId
     AND ac.GradoId = (SELECT CASE WHEN now() >= cn.fechainicios2 THEN MAX(g2.GradoId) ELSE MIN(g2.GradoId) END
                    FROM ce_alumnoporciclo ac2
                    INNER JOIN grado g2 ON ac2.GradoId = g2.GradoId
                    INNER JOIN ciclo c2 ON ac2.CicloId = c2.CicloId and ac2.CicloId = (SELECT CASE WHEN MAX(cactual.CicloId) IS NULL THEN max(acu.cicloid) ELSE  MAX(cactual.CicloId) END
                                                                      from ce_alumnoporciclo acu
                                                                      left JOIN ciclo cactual ON acu.CicloId = cactual.CicloId AND cactual.Actual = 1
                                                                      where acu.AlumnoId = a.alumnoid)
                    INNER JOIN ce_ciclopornivel cn ON c2.CicloId = cn.CicloId AND g2.NivelId = cn.NivelId
                    WHERE ac2.AlumnoId = a.AlumnoId)

inner join grado g on ac.GradoId = g.GradoId
inner join nivel n on g.NivelId = n.NivelId

left join ce_alumnocicloporgrupo acg on ac.AlumnoPorCicloId = acg.AlumnoPorCicloId
          AND acg.GrupoId = (SELECT gr2.GrupoId FROM ce_alumnocicloporgrupo acg2
                              INNER JOIN ce_grupo gr2 ON acg2.GrupoId = gr2.GrupoId AND gr2.TipoGrupoId = 1
                              WHERE acg2.AlumnoPorCicloId = ac.AlumnoPorCicloId)
left join ce_grupo gr on acg.GrupoId = gr.GrupoId

inner join cj_documentoporpagar d on a.AlumnoId = d.AlumnoId
where d.PagoEstatusId = 1 and d.Saldo > 0 and d.FechaLimitePago < now() and
acf.clavefamiliarid = $P{clavefamiliarid}
group by  acf.alumnoporclavefamiliar]]>
		</queryString>
		<field name="matricula" class="java.lang.String">
			<fieldDescription><![CDATA[MatrÝcula del alumno.]]></fieldDescription>
		</field>
		<field name="nombre" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="estatus" class="java.lang.String"/>
		<field name="nivel" class="java.lang.String"/>
		<field name="grado" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="grupo" class="java.lang.String"/>
		<field name="saldo" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[Saldo o adeudo restante del sub concepto.]]></fieldDescription>
		</field>
		<variable name="total" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{saldo}]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
	</subDataset>
	<subDataset name="padres" uuid="8ec6cba6-bad3-4bec-b740-5eddcde660ca">
		<parameter name="clavefamiliarid" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
concat_ws(' ', p.ApellidoPaterno, p.ApellidoMaterno, p.Nombre) as Nombre,
p.Telefono,
p.Celular,
concat_ws(' ', p.TelEmpresa, p.ExtensionEmpresa) as TelEmpresa
from  ce_padresotutoresclavefamiliar pcf
  inner join tutor t on pcf.TutorId = t.TutorId
inner join ce_padresotutores p on pcf.PadresOTutoresId = p.PadresOTutoresId
where pcf.ClaveFamiliarId = $P{clavefamiliarid}
order by t.TutorId]]>
