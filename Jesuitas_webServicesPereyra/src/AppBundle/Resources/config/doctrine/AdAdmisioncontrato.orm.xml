<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AdAdmisioncontrato" table="ad_admisioncontrato">
    <id name="contratoid" type="integer" column="Contratoid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="Nombre" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="orden" type="integer" column="Orden" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="extension" type="string" column="Extension" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tamano" type="integer" column="Tamano" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="documento" type="blob" column="Documento" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activo" type="boolean" column="Activo" nullable="true"/>
  </entity>
</doctrine-mapping>
