<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BcTipocredito" table="bc_tipocredito">
    <id name="tipocreditoid" type="integer" column="TipoCreditoId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="Descripcion" length="75" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activo" type="integer" column="Activo" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
