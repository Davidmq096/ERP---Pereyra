<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BrMostrarreactivos" table="br_mostrarreactivos">
    <id name="mostrarreactivosid" type="integer" column="MostrarReactivosId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="Descripcion" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cantidad" type="integer" column="Cantidad" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="activo" type="boolean" column="Activo" nullable="true"/>
  </entity>
</doctrine-mapping>
