<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BrSistemacalificacion" table="br_sistemacalificacion">
    <id name="sistemacalificacionid" type="integer" column="SistemaCalificacionId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="Nombre" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="inicio" type="integer" column="Inicio" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fin" type="integer" column="Fin" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="activo" type="boolean" column="Activo" nullable="true"/>
  </entity>
</doctrine-mapping>
