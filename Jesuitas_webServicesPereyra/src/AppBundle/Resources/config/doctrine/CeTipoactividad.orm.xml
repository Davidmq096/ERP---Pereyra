<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CeTipoactividad" table="ce_tipoactividad">
    <id name="tipoactividadid" type="integer" column="TipoActividadId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="Nombre" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activo" type="boolean" column="Activo" nullable="true"/>
    <field name="descripcionadmin" type="text" column="DescripcionAdmin" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcionalumno" type="text" column="DescripcionAlumno" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcionpadre" type="text" column="DescripcionPadre" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="boolean" column="Email" nullable="true"/>
    <field name="movil" type="boolean" column="Movil" nullable="true"/>
  </entity>
</doctrine-mapping>
