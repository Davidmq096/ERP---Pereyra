<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Ciclo" table="ciclo">
    <id name="cicloid" type="integer" column="CicloId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="Nombre" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="actual" type="boolean" column="Actual" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="activo" type="boolean" column="Activo" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="siguiente" type="boolean" column="Siguiente" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
