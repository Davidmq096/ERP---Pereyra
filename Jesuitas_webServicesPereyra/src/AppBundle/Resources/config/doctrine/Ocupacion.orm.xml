<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Ocupacion" table="ocupacion">
    <id name="ocupacionid" type="integer" column="OcupacionId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="clave" type="string" column="Clave" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clasificacion" type="text" column="Clasificacion" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcion" type="text" column="Descripcion" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activo" type="boolean" column="Activo" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
