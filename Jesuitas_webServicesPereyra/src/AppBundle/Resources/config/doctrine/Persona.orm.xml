<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Persona" table="persona">
    <id name="personaid" type="integer" column="PersonaId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="Nombre" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidopaterno" type="string" column="ApellidoPaterno" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidomaterno" type="string" column="ApellidoMaterno" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="Email" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="Telefono" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="celular" type="string" column="Celular" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecharegistro" type="datetime" column="FechaRegistro" nullable="true"/>
  </entity>
</doctrine-mapping>
